      "source": [
        "def calculator (a,b,symbol):\n",
        "  if symbol == '+':\n",
        "    print (a+b)\n",
        "  elif symbol == '-':\n",
        "    print(a-b)\n",
        "  elif symbol == '/':\n",
        "    print (a/b)\n",
        "  elif symbol == '*':\n",
        "    print(a*b)\n",
        "  else :\n",
        "    print('Please input straight operation!')"
      ]
    },
